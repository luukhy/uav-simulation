<?xml version="1.0"?>
<sdf version="1.5">
  <model name="parrot_bebop_2">
    <static>false</static>
    <pose>0 0 0.062656 0 0 0</pose>

    <!-- Main link -->
    <link name="body">
      <inertial>
        <mass>0.503</mass>
        <inertia>
          <ixx>1.805e-3</ixx> <ixy>-8.619e-7</ixy> <iyy>1.764e-3</iyy>
          <ixz>1.555e-5</ixz> <iyz>3.595e-7</iyz> <izz>3.328e-3</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <pose>-0.00424 0.00014 -0.00595 0 0 0</pose>
        <geometry><box><size>0.316618 0.330564 0.089582</size></box></geometry>
      </collision>
      <visual name="hull">
        <geometry><mesh><uri>model://parrot_bebop_2/meshes/hull.dae</uri></mesh></geometry>
      </visual>

      <sensor name="imu_sensor" type="imu">
        <always_on>1</always_on>
        <update_rate>250</update_rate>
        <visualize>true</visualize>
        
        <topic>/imu</topic> 
        
        <imu>
          <angular_velocity>
            <x><noise type="gaussian"><mean>0.0</mean><stddev>0.0</stddev></noise></x>
            <y><noise type="gaussian"><mean>0.0</mean><stddev>0.0</stddev></noise></y>
            <z><noise type="gaussian"><mean>0.0</mean><stddev>0.0</stddev></noise></z>
          </angular_velocity>
          <linear_acceleration>
            <x><noise type="gaussian"><mean>0.0</mean><stddev>0.0</stddev></noise></x>
            <y><noise type="gaussian"><mean>0.0</mean><stddev>0.0</stddev></noise></y>
            <z><noise type="gaussian"><mean>0.0</mean><stddev>0.0</stddev></noise></z>
          </linear_acceleration>
        </imu>
      </sensor>
    </link>

    <link name="propeller_rr">
      <pose>-0.09035 -0.11423 -0.00501 0 0 0</pose> 
      <inertial>
        <mass>0.005</mass> <inertia>
          <ixx>9.75e-07</ixx> <ixy>0</ixy> <iyy>0.000273104</iyy> <ixz>0</ixz> <iyz>0</iyz> <izz>0.000274004</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry><mesh><uri>model://parrot_bebop_2/meshes/propeller_rr.dae</uri></mesh></geometry>
      </visual>
    </link>
    <joint name="propeller_rr_joint" type="revolute">
      <parent>body</parent>
      <child>propeller_rr</child>
      <pose>0 0 0 0 0 0</pose> 
      <axis>
        <xyz>0 0 1</xyz>
        <limit><lower>-1e16</lower><upper>1e16</upper></limit>
      </axis>
    </joint>

    <link name="propeller_rl">
      <pose>-0.09035 0.11423 -0.00501 0 0 0</pose>
      <inertial>
        <mass>0.005</mass> <inertia><ixx>9.75e-07</ixx><ixy>0</ixy><iyy>0.000273104</iyy><ixz>0</ixz><iyz>0</iyz><izz>0.000274004</izz></inertia>
      </inertial>
      <visual name="visual">
        <geometry><mesh><uri>model://parrot_bebop_2/meshes/propeller_rl.dae</uri></mesh></geometry>
      </visual>
    </link>
    <joint name="propeller_rl_joint" type="revolute">
      <parent>body</parent> <child>propeller_rl</child>
      <pose>0 0 0 0 0 0</pose>
      <axis><xyz>0 0 1</xyz> <limit><lower>-1e16</lower><upper>1e16</upper></limit></axis>
    </joint>

    <link name="propeller_fr">
      <pose>0.09035 -0.11423 -0.00501 0 0 0</pose>
      <inertial>
        <mass>0.005</mass> <inertia><ixx>9.75e-07</ixx><ixy>0</ixy><iyy>0.000273104</iyy><ixz>0</ixz><iyz>0</iyz><izz>0.000274004</izz></inertia>
      </inertial>
      <visual name="visual">
        <geometry><mesh><uri>model://parrot_bebop_2/meshes/propeller_fr.dae</uri></mesh></geometry>
      </visual>
    </link>
    <joint name="propeller_fr_joint" type="revolute">
      <parent>body</parent> <child>propeller_fr</child>
      <pose>0 0 0 0 0 0</pose>
      <axis><xyz>0 0 1</xyz> <limit><lower>-1e16</lower><upper>1e16</upper></limit></axis>
    </joint>

    <link name="propeller_fl">
      <pose>0.09035 0.11423 -0.00501 0 0 0</pose>
      <inertial>
        <mass>0.005</mass> <inertia><ixx>9.75e-07</ixx><ixy>0</ixy><iyy>0.000273104</iyy><ixz>0</ixz><iyz>0</iyz><izz>0.000274004</izz></inertia>
      </inertial>
      <visual name="visual">
        <geometry><mesh><uri>model://parrot_bebop_2/meshes/propeller_fl.dae</uri></mesh></geometry>
      </visual>
    </link>
    <joint name="propeller_fl_joint" type="revolute">
      <parent>body</parent> <child>propeller_fl</child>
      <pose>0 0 0 0 0 0</pose>
      <axis><xyz>0 0 1</xyz> <limit><lower>-1e16</lower><upper>1e16</upper></limit></axis>
    </joint>

    <!-- plugins -->
    <plugin filename="gz-sim-multicopter-motor-model-system" name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>bebop</robotNamespace>
      <jointName>propeller_rr_joint</jointName> 
      <linkName>propeller_rr</linkName>      
      <turningDirection>ccw</turningDirection> 
      <timeConstantUp>0.0125</timeConstantUp> 
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>838</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.016</momentConstant>

      <commandSubTopic>command/motor_speed/prop_rr</commandSubTopic>
      <motorNumber>0</motorNumber>
      <motorSpeedPubTopic>motor_speed/prop_rr</motorSpeedPubTopic>

      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>

    <plugin filename="gz-sim-multicopter-motor-model-system" name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>bebop</robotNamespace>
      <jointName>propeller_fl_joint</jointName> 
      <linkName>propeller_fl</linkName>      
      <turningDirection>ccw</turningDirection> 
      <timeConstantUp>0.0125</timeConstantUp> 
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>838</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.016</momentConstant>

      <commandSubTopic>command/motor_speed/prop_fl</commandSubTopic>
      <motorNumber>0</motorNumber>
      <motorSpeedPubTopic>motor_speed/prop_fl</motorSpeedPubTopic>

      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>

    <plugin filename="gz-sim-multicopter-motor-model-system" name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>bebop</robotNamespace>
      <jointName>propeller_fr_joint</jointName> 
      <linkName>propeller_fr</linkName>      
      <turningDirection>cw</turningDirection> 
      <timeConstantUp>0.0125</timeConstantUp> 
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>838</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.016</momentConstant>

      <commandSubTopic>command/motor_speed/prop_fr</commandSubTopic>
      <motorNumber>0</motorNumber>
      <motorSpeedPubTopic>motor_speed/prop_fr</motorSpeedPubTopic>

      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>

    <plugin filename="gz-sim-multicopter-motor-model-system" name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>bebop</robotNamespace>
      <jointName>propeller_rl_joint</jointName> 
      <linkName>propeller_rl</linkName>      
      <turningDirection>cw</turningDirection> 
      <timeConstantUp>0.0125</timeConstantUp> 
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>838</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.016</momentConstant>

      <commandSubTopic>command/motor_speed/prop_rl</commandSubTopic>
      <motorNumber>0</motorNumber>
      <motorSpeedPubTopic>motor_speed/prop_rl</motorSpeedPubTopic>

      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>

    <plugin filename="gz-sim-sensors-system" name="gz::sim::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
    <plugin filename="gz-sim-imu-system" 
      name="gz::sim::systems::Imu">
    </plugin>

  </model>
</sdf>